{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    }
  ],
  "functions": {
    "api/*.ts": {
      "includeFiles": ["node_modules/tiny-secp256k1/lib/secp256k1.wasm"]
    }
  }
}
